{
  "properties": [
    {
      "name": "app.task.temp-clean.enabled",
      "type": "java.lang.String",
      "description": "临时文件清理任务开关."
    },
    {
      "name": "upload.mode",
      "type": "java.lang.String",
      "description": "文件上传模式：minio 或 oss.",
      "defaultValue": "oss"
    },
    {
      "name": "upload.minio.endpoint",
      "type": "java.lang.String",
      "description": "MinIO 服务端点地址."
    },
    {
      "name": "upload.minio.access-key",
      "type": "java.lang.String",
      "description": "MinIO 访问密钥."
    },
    {
      "name": "upload.minio.secret-key",
      "type": "java.lang.String",
      "description": "MinIO 私有密钥."
    },
    {
      "name": "upload.minio.bucket-name",
      "type": "java.lang.String",
      "description": "MinIO 存储桶名称."
    },
    {
      "name": "upload.oss.endpoint",
      "type": "java.lang.String",
      "description": "阿里云 OSS 服务端点地址."
    },
    {
      "name": "upload.oss.access-key-id",
      "type": "java.lang.String",
      "description": "阿里云 OSS AccessKey ID."
    },
    {
      "name": "upload.oss.access-key-secret",
      "type": "java.lang.String",
      "description": "阿里云 OSS AccessKey Secret."
    },
    {
      "name": "upload.oss.bucket-name",
      "type": "java.lang.String",
      "description": "阿里云 OSS 存储桶名称."
    },
    {
      "name": "upload.oss.custom-domain",
      "type": "java.lang.String",
      "description": "阿里云 OSS 自定义域名."
    },
    {
      "name": "springdoc.info.title",
      "type": "java.lang.String",
      "description": "API 文档标题.",
      "defaultValue": "API"
    },
    {
      "name": "springdoc.info.description",
      "type": "java.lang.String",
      "description": "API 文档描述."
    },
    {
      "name": "springdoc.info.version",
      "type": "java.lang.String",
      "description": "API 版本号.",
      "defaultValue": "1.0.0"
    },
    {
      "name": "webp.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用WebP自动转换.",
      "defaultValue": true
    },
    {
      "name": "webp.quality",
      "type": "java.lang.Float",
      "description": "WebP压缩质量 (0.0-1.0)，0.85为推荐值.",
      "defaultValue": 0.85
    },
    {
      "name": "webp.max-width",
      "type": "java.lang.Integer",
      "description": "图片最大宽度（像素），超过此尺寸会自动缩放.",
      "defaultValue": 2048
    },
    {
      "name": "webp.max-height",
      "type": "java.lang.Integer",
      "description": "图片最大高度（像素），超过此尺寸会自动缩放.",
      "defaultValue": 2048
    },
    {
      "name": "webp.keep-original",
      "type": "java.lang.Boolean",
      "description": "是否保留原图（false推荐，节省存储空间）.",
      "defaultValue": false
    },
    {
      "name": "webp.failure-strategy",
      "type": "java.lang.String",
      "description": "转换失败时的处理策略.",
      "defaultValue": "KEEP_ORIGINAL"
    },
    {
      "name": "webp.enable-stats",
      "type": "java.lang.Boolean",
      "description": "是否记录转换统计信息.",
      "defaultValue": true
    }
  ],
  "hints": [
    {
      "name": "upload.mode",
      "values": [
        {
          "value": "minio",
          "description": "使用 MinIO 对象存储."
        },
        {
          "value": "oss",
          "description": "使用阿里云 OSS 对象存储."
        }
      ]
    },
    {
      "name": "webp.failure-strategy",
      "values": [
        {
          "value": "KEEP_ORIGINAL",
          "description": "转换失败时保留原图."
        },
        {
          "value": "THROW_EXCEPTION",
          "description": "转换失败时抛出异常."
        }
      ]
    }
  ]
}